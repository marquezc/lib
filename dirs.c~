#include <stdio.h>
#include <stdlib.h>
#include <dirent.h>

int
main(int argc, char *argv[])
{
  DIR   *dp;
  struct dirent *dirp;

  if (argc != 2)
    {
      printf("usgerr: argument (directory name) required\n");
      exit(1);
    }

  if ((dp = opendir(argv[1])) == NULL)
    printf("syserr: can't open %s\n", argv[1]);

  while ((dirp = readdir(dp)) != NULL)
    {

    }
    }
